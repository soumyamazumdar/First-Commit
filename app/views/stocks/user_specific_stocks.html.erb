<h1>user_specific_stocks</h1>


<% Company.find_by_id(@comp).stocks.each do |i| %>
  <% current_user.stocks.select(:stock_id).distinct.each do |thing| %>
    <% @sid = thing.stock_id %>
    <% if i.id==@sid %>
      <%= Stock.find(@sid).name %>
      <%= link_to 'Show', transactions_stockwise_total_transaction_path(s: i.id) %><br />
      <%#= link_to 'Sell' %>
    <% end %>
  <% end %>  
<% end %>	

<%= link_to "Back", transactions_companywise_stocks_bought_path %>